#include<iostream>
#include<unordered_map>
#include<map>
using namespace std;

int main(){
    map<string,int>m;
    m["china"]=150;
    m["india"]=150;
    m["us"]=50;
    m["nepal"]= 10;
    //to change value
    m["nepal"]= 200;

   // cout<< m["india"]<< endl;

   for(pair<string,int> country : m){
    cout << country.first<<","<< country.second << endl;
   }
    m.erase("nepal");//erase

   if(m.count("nepal")){
    cout <<"nepal exists\n";
   }else{
    cout<<"nepal doesnt exist\n";
   }
   //count if 0 not exist

   
    return 0;
}
