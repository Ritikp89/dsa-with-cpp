#include<iostream>
#include<unordered_map>
#include<map>
#include<unordered_set>
#include<set>
using namespace std;

int main(){
  set<int> s;

    s.insert(1);
    s.insert(5);
    s.insert(3);
    s.insert(2);
   
     
    cout <<s.size()<< endl;
    s.erase(3);

    if(s.find(3)!= s.end()){
        cout<<"3 exist";
    }else{
        cout<<"3 doesnt exist";
    }
    for(auto el : s){
        cout<<el<<" ";
    }
    return 0;
}
